  "version": "1.0.0",
  "description": "Enterprise Agent Supervisor - Rules Engine MCP Server for AI Agent Governance",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-supervisor": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts",
